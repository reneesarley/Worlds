@{
    ViewData["Title"] = "Country Selector";
}
        
<br>
<form name="select" action="/result">
<button class="btn btn-primary" type="submit">
    Dropdown button
  </button>
<select name="selection">
    @foreach (var option in Model)
    {
       <option value="@option">@option</option>
    }
</select>

</form>